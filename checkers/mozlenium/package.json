  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-firefox-dependencies": "handpick --target=firefoxDependencies",
    "install-chrome-dependencies": "handpick --target=chromeDependencies",
    "firefox:install": "IS_CLI=true SSO_LDAP_PASSWORD=$npm_config_ldap_pw ./install.sh firefox",
    "chrome:install": "IS_CLI=true SSO_LDAP_PASSWORD=$npm_config_ldap_pw ./install.sh chrome",
    "firefox:install:test": "npm run install:firefox && ./run.sh firefox $npm_config_test",
    "chrome:install:test": "npm run install:chrome && ./run.sh chrome $npm_config_test",
    "firefox:test": "IS_CLI=true SSO_LDAP_PASSWORD=$npm_config_ldap_pw ./run.sh firefox $npm_config_test",
    "chrome:test": "IS_CLI=true SSO_LDAP_PASSWORD=$npm_config_ldap_pw ./run.sh chrome $npm_config_test"
